/* cerulean, cosmo, cyborg, darkly, flatly, journal, litera, lumen, lux, materia, minty, morph,
   pulse, quartz, sandstone, simplex, sketchy, slate, solar, spacelab, superhero, united, vapor,
   yeti, zephyr */
<%= RSpecDocumentation.theme(:materia) %>

/* Base16, BlackWhiteTheme, Colorful, Github, Gruvbox, IgorPro, Magritte, Molokai, Monokai,
   MonokaiSublime, Pastie, ThankfulEyes, Tulip */
<%= Rouge::Theme.find('molokai').render(scope: '.highlight') %>

/* https://stackoverflow.com/a/61587938 */
.consistent-height .tab-content {
  display: flex;
}

.consistent-height .tab-content > .tab-pane {
  display: block; /* undo "display: none;" */
  visibility: hidden;
  margin-right: -100%;
  width: 100%;
}

.consistent-height .tab-content > .active {
  visibility: visible;
}

@keyframes fade-in {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

body {
  opacity: 0;
  animation: fade-in ease-in 1;
  animation-fill-mode: forwards;
  animation-duration: 0.15s;
  animation-delay: .1s
}

.header-wrapper {
  background-color: #fff;
}

table.rspec-documentation-table td, table.rspec-documentation-table th {
  border: 1px solid #cdcdcd;
  padding: 0.5rem 1rem;
}

[data-bs-theme="dark"] table.rspec-documentation-table td, [data-bs-theme="dark"] table.rspec-documentation-table th {
  border: 1px solid #4e4e4e;
}

[data-bs-theme="dark"] .header-wrapper {
  background-color: #212121;
}

.theme-switcher {
  display: inline-block;
  vertical-align: middle;
  margin-bottom: 1rem;
  cursor: pointer;
}

.theme-icon {
  width: 1.5rem;
  height: 1.5rem;
}

.theme-light-icon path {
  fill: #fcdb33;
}

.theme-dark-icon path {
  fill: #63afc9;
}

.content h1 {
  padding-top: 1rem;
}

h1.title a {
  text-decoration: none;
}

.code {
  font-family: monospace;
  max-height: 30rem;
  overflow-y: auto;
}

pre {
  padding: 1rem;
  border-radius: 10px;
}

div.language-console {
  margin: 1rem;
}

div.branding {
  text-align: right;
}

h1.title {
  display: inline;
}

.hidden {
  visibility: hidden;
}

.modal-body .row .col {
  padding-top: 1rem;
}

.page-tree ol, ol.page-tree {
  margin: 0;
  list-style-type: none;
  padding-left: 1rem;
}

ol.page-tree {
  padding-left: 0;
}

.page-tree li {
  line-height: 1.8rem;
  white-space: nowrap;
}

.page-tree li.active {
  list-style-type: square;
}

.page-tree li a {
  text-decoration: none;
}

.page-tree li a.active {
  font-weight: bold;
}

.header .separator {
  display: inline-block;
  height: 2.5rem;
}

.version {
  font-size: 2rem;
}

div.highlight {
  padding: 1rem;
  padding-bottom: 0.05rem;
  border-radius: 10px;
  margin-bottom: 2rem;
}

.highlight .go {
  color: #706d6d;
}

